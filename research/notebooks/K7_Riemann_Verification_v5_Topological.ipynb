    "# ============================================================\n",
    "# FINAL TEST: Pure GIFT Formula\n",
    "# ============================================================\n",
    "\n",
    "print(\"=\"*70)\n",
    "print(\"FINAL TEST: PURE GIFT FORMULAS\")\n",
    "print(\"=\"*70)\n",
    "\n",
    "# Based on analysis, test the most promising pure GIFT formulas\n",
    "\n",
    "# Formula 1: Œ≥‚Çô = Œ≥‚ÇÅ √ó n^((b‚ÇÉ-b‚ÇÇ)/H*) with log correction\n",
    "exp1 = (B3 - B2) / H_STAR  # 56/99\n",
    "gamma_f1 = gamma_1 * n_vals ** exp1 * (1 + 0.1 * np.log(n_vals + 1))\n",
    "err_f1 = np.mean(np.abs(gamma_f1 - RIEMANN_ZEROS) / RIEMANN_ZEROS * 100)\n",
    "\n",
    "# Formula 2: Œ≥‚Çô = Œ≥‚ÇÅ √ó n^(b‚ÇÇ/b‚ÇÉ) √ó H*/b‚ÇÉ\n",
    "exp2 = B2/B3\n",
    "gamma_f2 = gamma_1 * n_vals ** exp2 * (H_STAR/B3) ** (np.log(n_vals)/10)\n",
    "err_f2 = np.mean(np.abs(gamma_f2 - RIEMANN_ZEROS) / RIEMANN_ZEROS * 100)\n",
    "\n",
    "# Formula 3: Œ≥‚Çô = (H*/dim(G‚ÇÇ)) √ó n √ó log(n + dim(G‚ÇÇ)) / log(H*)\n",
    "gamma_f3 = (H_STAR/DIM_G2) * n_vals * np.log(n_vals + DIM_G2) / np.log(H_STAR)\n",
    "err_f3 = np.mean(np.abs(gamma_f3 - RIEMANN_ZEROS) / RIEMANN_ZEROS * 100)\n",
    "\n",
    "# Formula 4: Riemann-von Mangoldt style with GIFT constants\n",
    "# N(T) ~ T/(2œÄ) √ó log(T/(2œÄe))\n",
    "# Inverted: Œ≥‚Çô ~ 2œÄn / log(n)\n",
    "# GIFT version: Œ≥‚Çô ~ (H*/dim(G‚ÇÇ)) √ó n / log(n + 1) √ó correction\n",
    "gamma_f4 = (H_STAR/DIM_G2) * n_vals / np.log(n_vals + np.e) * np.log(H_STAR)\n",
    "err_f4 = np.mean(np.abs(gamma_f4 - RIEMANN_ZEROS) / RIEMANN_ZEROS * 100)\n",
    "\n",
    "print(f\"\\n{'Formula':<60} | {'Mean Err%':>10}\")\n",
    "print(\"-\"*75)\n",
    "print(f\"{'Œ≥‚ÇÅ √ó n^((b‚ÇÉ-b‚ÇÇ)/H*) √ó (1 + 0.1√ólog(n+1))':<60} | {err_f1:10.2f}%\")\n",
    "print(f\"{'Œ≥‚ÇÅ √ó n^(b‚ÇÇ/b‚ÇÉ) √ó (H*/b‚ÇÉ)^(log(n)/10)':<60} | {err_f2:10.2f}%\")\n",
    "print(f\"{'(H*/dim(G‚ÇÇ)) √ó n √ó log(n+dim(G‚ÇÇ)) / log(H*)':<60} | {err_f3:10.2f}%\")\n",
    "print(f\"{'(H*/dim(G‚ÇÇ)) √ó n / log(n+e) √ó log(H*)':<60} | {err_f4:10.2f}%\")"
   ]

    "print(f\"  b = {b_empirical:.6f}\")\n",
    "\n",
    "# Find closest GIFT ratio to b\n",
    "print(f\"\\nClosest GIFT ratios to b = {b_empirical:.6f}:\")\n",
    "gift_ratios = {\n",
    "    'b‚ÇÇ/b‚ÇÉ': B2/B3,\n",
    "    '2/7 (Weyl)': 2/7,\n",
    "    '(b‚ÇÇ+1)/b‚ÇÉ': (B2+1)/B3,\n",
    "    'b‚ÇÇ/H*': B2/H_STAR,\n",
    "    'dim(G‚ÇÇ)/b‚ÇÉ': DIM_G2/B3,\n",
    "    '(b‚ÇÉ-b‚ÇÇ)/b‚ÇÉ': (B3-B2)/B3,\n",
    "    'dim(K‚Çá)/b‚ÇÇ': DIM_K7/B2,\n",
    "}\n",
    "\n",
    "for name, val in sorted(gift_ratios.items(), key=lambda x: abs(x[1] - b_empirical)):\n",
    "    diff = abs(val - b_empirical) / b_empirical * 100\n",
    "    print(f\"  {name:<20} = {val:.6f} (diff: {diff:.2f}%)\")\n",
    "\n",
    "# The key insight: what combination gives b?\n",
    "print(f\"\\nüîç Searching for GIFT formula that gives b = {b_empirical:.6f}...\")\n",
    "\n",
    "# Test various combinations\n",
    "formulas = [\n",
    "    ('b‚ÇÇ/b‚ÇÉ', B2/B3),\n",
    "    ('(b‚ÇÇ+dim(G‚ÇÇ))/(b‚ÇÉ+H*)', (B2+DIM_G2)/(B3+H_STAR)),\n",
    "    ('dim(K‚Çá)/(b‚ÇÇ+dim(G‚ÇÇ))', DIM_K7/(B2+DIM_G2)),\n",
    "    ('(b‚ÇÇ-dim(G‚ÇÇ))/b‚ÇÇ', (B2-DIM_G2)/B2),\n",
    "    ('b‚ÇÇ/(b‚ÇÉ+b‚ÇÇ)', B2/(B3+B2)),\n",
    "    ('(H*-b‚ÇÉ)/b‚ÇÇ', (H_STAR-B3)/B2),\n",
    "    ('dim(G‚ÇÇ)/(b‚ÇÉ-b‚ÇÇ)', DIM_G2/(B3-B2)),\n",
    "    ('(b‚ÇÉ-b‚ÇÇ)/H*', (B3-B2)/H_STAR),\n",
    "]\n",
    "\n",
    "print(f\"\\n{'Formula':<35} | {'Value':>10} | {'vs b':>10}\")\n",
    "print(\"-\"*60)\n",
    "for name, val in sorted(formulas, key=lambda x: abs(x[1] - b_empirical)):\n",
    "    diff = (val - b_empirical) / b_empirical * 100\n",
    "    print(f\"{name:<35} | {val:10.6f} | {diff:+9.2f}%\")"
   ]

    "# ============================================================\n",
    "# Refined Formula Search\n",
    "# ============================================================\n",
    "\n",
    "print(\"=\"*70)\n",
    "print(\"REFINED FORMULA SEARCH\")\n",
    "print(\"=\"*70)\n",
    "\n",
    "# The empirical exponent is ~0.58-0.60\n",
    "# Let's find a GIFT formula that matches\n",
    "\n",
    "# Key observation: b_empirical ‚âà 0.58\n",
    "# Close candidates:\n",
    "# - (b‚ÇÉ - b‚ÇÇ) / H* = 56/99 = 0.5657\n",
    "# - dim(G‚ÇÇ) / b‚ÇÇ = 14/21 = 0.6667\n",
    "# - b‚ÇÉ / (b‚ÇÇ + b‚ÇÉ + dim(G‚ÇÇ)) = 77/112 = 0.6875\n",
    "\n",
    "# Try: maybe it's a combination!\n",
    "# Or: the formula involves TWO terms\n",
    "\n",
    "# Test: Œ≥‚Çô = Œ≥‚ÇÅ √ó n^a √ó (1 + c√ólog(n))\n",
    "# This is closer to the actual Riemann-von Mangoldt formula\n",
    "\n",
    "from scipy.optimize import curve_fit\n",
    "\n",
    "def riemann_approx(n, a, b, c):\n",
    "    \"\"\"Œ≥‚Çô ‚âà a √ó n^b √ó (1 + c/log(n+1))\"\"\"\n",
    "    return a * n**b * (1 + c / np.log(n + 1))\n",
    "\n",
    "# Fit\n",
    "popt, _ = curve_fit(riemann_approx, n_vals, RIEMANN_ZEROS, p0=[14, 0.6, 0.1])\n",
    "a_fit, b_fit, c_fit = popt\n",
    "\n",
    "gamma_refined = riemann_approx(n_vals, a_fit, b_fit, c_fit)\n",
    "errors_refined = np.abs(gamma_refined - RIEMANN_ZEROS) / RIEMANN_ZEROS * 100\n",
    "\n",
    "print(f\"\\nRefined formula: Œ≥‚Çô = {a_fit:.4f} √ó n^{b_fit:.4f} √ó (1 + {c_fit:.4f}/log(n+1))\")\n",
    "print(f\"Mean error: {np.mean(errors_refined):.2f}%\")\n",
    "print(f\"Max error: {np.max(errors_refined):.2f}%\")\n",
    "\n",
    "# Check if parameters relate to GIFT\n",
    "print(f\"\\nParameter analysis:\")\n",
    "print(f\"  a = {a_fit:.4f} vs Œ≥‚ÇÅ = {gamma_1:.4f} (ratio: {a_fit/gamma_1:.4f})\")\n",
    "print(f\"  b = {b_fit:.4f} vs (b‚ÇÉ-b‚ÇÇ)/H* = {(B3-B2)/H_STAR:.4f}\")\n",
    "print(f\"  b = {b_fit:.4f} vs b‚ÇÉ/(b‚ÇÇ+b‚ÇÉ+dim(G‚ÇÇ)) = {B3/(B2+B3+DIM_G2):.4f}\")"
   ]

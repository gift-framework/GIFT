{
  "encoding_type": "fourier",
  "hidden_dims": [
    256,
    256,
    128
  ],
  "fourier_modes": 16,
  "fourier_scale": 1.0,
  "omega_0": 30.0,
  "normalize_phi": true,
  "manifold_type": "T7",
  "batch_size": 512,
  "epochs": 3000,
  "learning_rate": 0.0001,
  "weight_decay": 0.0001,
  "grad_clip": 1.0,
  "checkpoint_interval": 500,
  "validation_interval": 500,
  "device": "cpu",
  "seed": 42,
  "output_dir": "checkpoints",
  "use_curriculum": true,
  "derivative_method": "autograd",
  "use_scheduler": true,
  "scheduler_T0": 500,
  "scheduler_eta_min": 1e-07
}
{
  "version": "v1.0",
  "implementation": "complete_tcs_with_calibration_and_yukawa",
  "target_metrics": {
    "torsion_closure": 1e-3,
    "torsion_coclosure": 1e-3,
    "yukawa_deviation": 0.1,
    "det_gram_h2": 1.0,
    "det_gram_h3": 1.0
  },
  "architecture": {
    "phi_network": {
      "hidden_dims": [384, 384, 256],
      "n_fourier": 32,
      "activation": "silu"
    },
    "harmonic_h2_network": {
      "hidden_dim": 128,
      "n_fourier": 24,
      "n_forms": 21
    },
    "harmonic_h3_network": {
      "hidden_dim": 128,
      "n_fourier": 24,
      "n_forms": 77
    }
  },
  "gift_parameters": {
    "tau": 3.8967452300785634,
    "xi": 0.9817477042468103,
    "gamma": 0.5780542986425339,
    "phi": 1.618033988749895,
    "beta0": 0.39269908169872414,
    "delta": 0.25132741228718347,
    "epsilon0": 0.125,
    "b2": 21,
    "b3": 77,
    "chi": 0,
    "h_star": 99,
    "b2_m1": 11,
    "b3_m1": 40,
    "b2_m2": 10,
    "b3_m2": 37,
    "dim_su3": 8,
    "dim_su2": 3,
    "dim_u1": 1,
    "dim_hidden": 9
  },
  "topology": {
    "type": "complete_K7_acyl_neck_acyl",
    "n_regions": 3,
    "grid_resolution_schedule": {
      "phase1": 8,
      "phase2": 8,
      "phase3": 10,
      "phase4": 10,
      "phase5": 12,
      "final_validation": 14
    }
  },
  "calibration_cycles": {
    "associative_3cycles": {
      "n_cycles": 6,
      "samples_per_cycle": 512,
      "regions": ["M1", "neck", "M2"]
    },
    "coassociative_4cycles": {
      "n_cycles": 6,
      "samples_per_cycle": 512,
      "regions": ["M1", "neck", "M2"]
    }
  },
  "training": {
    "total_epochs": 15000,
    "batch_size": 2048,
    "grad_accumulation_steps": 4,
    "effective_batch_size": 8192,
    "optimizer": "AdamW",
    "base_lr": 1e-4,
    "weight_decay": 1e-4,
    "grad_clip": 1.0,
    "warmup_epochs": 500,
    "curriculum": {
      "phase1_neck_stability": {
        "range": [0, 2000],
        "grid_n": 8,
        "region_weights": {
          "m1": 0.2,
          "neck": 0.6,
          "m2": 0.2
        },
        "loss_weights": {
          "torsion_closure": 0.5,
          "torsion_coclosure": 0.5,
          "volume": 2.0,
          "gram_h2": 1.0,
          "gram_h3": 0.5,
          "boundary": 0.5,
          "calibration": 0.0
        }
      },
      "phase2_acyl_matching": {
        "range": [2000, 5000],
        "grid_n": 8,
        "region_weights": {
          "m1": 0.3,
          "neck": 0.4,
          "m2": 0.3
        },
        "loss_weights": {
          "torsion_closure": 1.0,
          "torsion_coclosure": 1.0,
          "volume": 0.5,
          "gram_h2": 1.5,
          "gram_h3": 1.0,
          "boundary": 1.5,
          "calibration": 0.0
        }
      },
      "phase3_cohomology_refinement": {
        "range": [5000, 8000],
        "grid_n": 10,
        "region_weights": {
          "m1": 0.25,
          "neck": 0.5,
          "m2": 0.25
        },
        "loss_weights": {
          "torsion_closure": 2.0,
          "torsion_coclosure": 2.0,
          "volume": 0.2,
          "gram_h2": 3.0,
          "gram_h3": 2.0,
          "boundary": 2.0,
          "calibration": 0.5
        }
      },
      "phase4_harmonic_extraction": {
        "range": [8000, 10000],
        "grid_n": 10,
        "region_weights": {
          "m1": 0.25,
          "neck": 0.5,
          "m2": 0.25
        },
        "loss_weights": {
          "torsion_closure": 3.0,
          "torsion_coclosure": 3.0,
          "volume": 0.1,
          "gram_h2": 5.0,
          "gram_h3": 3.0,
          "boundary": 1.5,
          "calibration": 1.0
        }
      },
      "phase5_calibration_finetune": {
        "range": [10000, 15000],
        "grid_n": 12,
        "region_weights": {
          "m1": 0.25,
          "neck": 0.5,
          "m2": 0.25
        },
        "loss_weights": {
          "torsion_closure": 5.0,
          "torsion_coclosure": 5.0,
          "volume": 0.05,
          "gram_h2": 5.0,
          "gram_h3": 4.0,
          "boundary": 1.0,
          "calibration": 3.0
        },
        "lr_schedule": {
          "type": "cosine_annealing",
          "lr_start": 1e-5,
          "lr_end": 1e-7
        }
      }
    }
  },
  "adaptive_loss": {
    "enabled": true,
    "check_interval": 100,
    "plateau_threshold": 1e-4,
    "boost_factor": 1.5,
    "reduce_factor": 0.8,
    "components": ["torsion_closure", "torsion_coclosure"]
  },
  "checkpointing": {
    "interval": 500,
    "keep_best_n": 5,
    "auto_resume": true,
    "save_components": ["phi_network", "harmonic_h2_network", "harmonic_h3_network", "optimizer", "scheduler", "metrics"]
  },
  "validation": {
    "interval": 100,
    "ricci_check_interval": 500,
    "ricci_test_points": 1000,
    "gram_svd_interval": 500,
    "components": [
      "torsion_closure",
      "torsion_coclosure",
      "det_gram_h2",
      "det_gram_h3",
      "rank_h2",
      "rank_h3",
      "ricci_scalar_norm",
      "calibration_error"
    ]
  },
  "yukawa_computation": {
    "enabled": true,
    "n_mc_samples": 20000,
    "grid_n": 10,
    "use_dual_method": true,
    "tucker_rank": [3, 3, 3],
    "antisymmetry_tolerance": 1e-6
  },
  "holonomy_test": {
    "enabled": true,
    "n_loops": 10,
    "n_steps_per_loop": 50,
    "preservation_tolerance": 1e-4
  },
  "export": {
    "formats": ["pt", "npy", "json"],
    "save_history": true,
    "save_visualization_data": true,
    "compression": false
  },
  "seed": 42,
  "device": "cuda",
  "dtype": "float32"
}
